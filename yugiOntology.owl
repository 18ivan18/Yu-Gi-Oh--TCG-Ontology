<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://myonto.com/yugiOntology.owl"
         xmlns="http://myonto.com/yugiOntology.owl#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#">

<owl:Ontology rdf:about="http://myonto.com/yugiOntology.owl"/>

<owl:ObjectProperty rdf:about="#effect">
  <rdfs:domain rdf:resource="#Card"/>
  <rdfs:range rdf:resource="#Effect"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_type">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Monster"/>
  <rdfs:range rdf:resource="#Type"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_ability">
  <rdfs:domain rdf:resource="#Monster"/>
  <rdfs:range rdf:resource="#Ability"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_attribute">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Monster"/>
  <rdfs:range rdf:resource="#Attribute"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_rounds">
  <rdfs:domain rdf:resource="#Tournament"/>
  <rdfs:range rdf:resource="#Round"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_matches">
  <rdfs:domain rdf:resource="#Round"/>
  <rdfs:range rdf:resource="#Match"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_players">
  <rdfs:domain rdf:resource="#Match"/>
  <rdfs:range rdf:resource="#Player"/>
  <owl:inverseOf rdf:resource="#plays_in_match"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#plays_in_match">
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="#Match"/>
  <owl:inverseOf rdf:resource="#has_players"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#uses">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="#Deck"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#is_used_in">
  <rdfs:domain rdf:resource="#Card"/>
  <rdfs:range rdf:resource="#Deck"/>
  <owl:inverseOf rdf:resource="#has_cards"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_cards">
  <rdfs:domain rdf:resource="#Deck"/>
  <rdfs:range rdf:resource="#Card"/>
  <owl:inverseOf rdf:resource="#is_used_in"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_winner">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Match"/>
  <rdfs:range rdf:resource="#Player"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#cossyId">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <owl:withRestrictions>
        <rdf:Description>
          <rdf:first>
            <rdf:Description>
              <xsd:pattern rdf:datatype="http://www.w3.org/2001/XMLSchema#string">\d{10}</xsd:pattern>
            </rdf:Description>
          </rdf:first>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
      </owl:withRestrictions>
    </rdfs:Datatype>
  </rdfs:range>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#participates_in">
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="#Tournament"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#has_text">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Effect"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#rank">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Monster"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#link_rating">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Monster"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#level">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Monster"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#defense">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Monster"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#atk">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Monster"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#name">
  <rdfs:domain rdf:resource="#Card"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#rarity">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Card"/>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:oneOf>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rare</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Super Rare</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ultra Rare</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ultimate Rare</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ghost Rare</rdf:first>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
        </rdf:rest>
        </rdf:Description>
        </rdf:rest>
        </rdf:Description>
        </rdf:rest>
        </rdf:Description>
        </rdf:rest>
        </rdf:Description>
        </rdf:rest>
        </rdf:Description>
      </owl:oneOf>
    </rdfs:Datatype>
  </rdfs:range>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#archtype">
  <rdfs:domain rdf:resource="#Deck"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#has_points">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#number_of_wins">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#number_of_draws">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#years_experience">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Card">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Monster"/>
        <rdf:Description rdf:about="#Spell"/>
        <rdf:Description rdf:about="#Trap"/>
      </owl:unionOf>
    </owl:Class>
  </owl:equivalentClass>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#name"/>
      <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:Restriction>
  </owl:equivalentClass>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">There are three main types of cards: Monster , Spell, Trap</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Yu-Gi-Oh TCG card</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Monster">
  <rdfs:subClassOf rdf:resource="#Card"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#effect"/>
      <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:maxCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Spell">
  <rdfs:subClassOf rdf:resource="#Card"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#effect"/>
      <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <owl:equivalentClass>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#ContiniousSpell"/>
        <rdf:Description rdf:about="#NormalSpell"/>
        <rdf:Description rdf:about="#EquipSpell"/>
        <rdf:Description rdf:about="#QuickPlaySpell"/>
        <rdf:Description rdf:about="#RitualSpell"/>
        <rdf:Description rdf:about="#FieldSpell"/>
      </owl:unionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Trap">
  <rdfs:subClassOf rdf:resource="#Card"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#effect"/>
      <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <owl:equivalentClass>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#CounterTrap"/>
        <rdf:Description rdf:about="#NormalTrap"/>
        <rdf:Description rdf:about="#ContiniousTrap"/>
      </owl:unionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Effect">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#NonEffectMonster">
  <rdfs:subClassOf rdf:resource="#Monster"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Monster"/>
        <owl:Class>
          <owl:complementOf>
            <owl:Restriction>
              <owl:onProperty rdf:resource="#effect"/>
              <owl:someValuesFrom rdf:resource="#Effect"/>
            </owl:Restriction>
          </owl:complementOf>
        </owl:Class>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#XyzMonster">
  <rdfs:subClassOf rdf:resource="#Monster"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#level"/>
      <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:cardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Monster"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#rank"/>
          <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#LinkMonster">
  <rdfs:subClassOf rdf:resource="#Monster"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#level"/>
      <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:cardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#defense"/>
      <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:cardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Monster"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#link_rating"/>
          <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#EffectMonster">
  <rdfs:subClassOf rdf:resource="#Monster"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Monster"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#effect"/>
          <owl:onClass rdf:resource="#Effect"/>
          <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#PendulumMonster">
  <rdfs:subClassOf rdf:resource="#Monster"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Monster"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#effect"/>
          <owl:onClass rdf:resource="#Effect"/>
          <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#FusionMonster">
  <rdfs:subClassOf rdf:resource="#Monster"/>
</owl:Class>

<owl:Class rdf:about="#SynchroMonster">
  <rdfs:subClassOf rdf:resource="#Monster"/>
</owl:Class>

<owl:Class rdf:about="#RitualMonster">
  <rdfs:subClassOf rdf:resource="#Monster"/>
</owl:Class>

<owl:Class rdf:about="#NormalMonster">
  <rdfs:subClassOf rdf:resource="#NonEffectMonster"/>
</owl:Class>

<owl:Class rdf:about="#Type">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:oneOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Dragon"/>
        <rdf:Description rdf:about="#Fairy"/>
        <rdf:Description rdf:about="#Machine"/>
        <rdf:Description rdf:about="#Warrior"/>
        <rdf:Description rdf:about="#Spellcaster"/>
      </owl:oneOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Ability">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:oneOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Union"/>
        <rdf:Description rdf:about="#Tuner"/>
        <rdf:Description rdf:about="#Spirit"/>
        <rdf:Description rdf:about="#Toon"/>
        <rdf:Description rdf:about="#Gemini"/>
        <rdf:Description rdf:about="#Flip"/>
      </owl:oneOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Attribute">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:oneOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Dark"/>
        <rdf:Description rdf:about="#Light"/>
        <rdf:Description rdf:about="#Divine"/>
        <rdf:Description rdf:about="#Earth"/>
        <rdf:Description rdf:about="#Fire"/>
        <rdf:Description rdf:about="#Water"/>
        <rdf:Description rdf:about="#Wind"/>
      </owl:oneOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#ContiniousSpell">
  <rdfs:subClassOf rdf:resource="#Spell"/>
</owl:Class>

<owl:Class rdf:about="#NormalSpell">
  <rdfs:subClassOf rdf:resource="#Spell"/>
</owl:Class>

<owl:Class rdf:about="#EquipSpell">
  <rdfs:subClassOf rdf:resource="#Spell"/>
</owl:Class>

<owl:Class rdf:about="#QuickPlaySpell">
  <rdfs:subClassOf rdf:resource="#Spell"/>
</owl:Class>

<owl:Class rdf:about="#RitualSpell">
  <rdfs:subClassOf rdf:resource="#Spell"/>
</owl:Class>

<owl:Class rdf:about="#FieldSpell">
  <rdfs:subClassOf rdf:resource="#Spell"/>
</owl:Class>

<owl:Class rdf:about="#ContiniousTrap">
  <rdfs:subClassOf rdf:resource="#Trap"/>
</owl:Class>

<owl:Class rdf:about="#NormalTrap">
  <rdfs:subClassOf rdf:resource="#Trap"/>
</owl:Class>

<owl:Class rdf:about="#CounterTrap">
  <rdfs:subClassOf rdf:resource="#Trap"/>
</owl:Class>

<owl:Class rdf:about="#Deck">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Person">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Player">
  <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<owl:Class rdf:about="#Match">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#has_players"/>
      <owl:onClass rdf:resource="#Player"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Round">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Event">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Tournament">
  <rdfs:subClassOf rdf:resource="#Event"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Locals"/>
        <rdf:Description rdf:about="#Regionals"/>
        <rdf:Description rdf:about="#WorldChampionshipQualifier"/>
      </owl:unionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Locals">
  <rdfs:subClassOf rdf:resource="#Tournament"/>
</owl:Class>

<owl:Class rdf:about="#Regionals">
  <rdfs:subClassOf rdf:resource="#Tournament"/>
</owl:Class>

<owl:Class rdf:about="#WorldChampionshipQualifier">
  <rdfs:subClassOf rdf:resource="#Tournament"/>
</owl:Class>

<owl:Class rdf:about="#CrowdFavourite">
  <rdfs:subClassOf rdf:resource="#Player"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Player"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#years_experience"/>
          <owl:allValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5</xsd:minExclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:allValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#ExperiencedPlayer">
  <rdfs:subClassOf rdf:resource="#Player"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#years_experience"/>
      <owl:allValuesFrom>
        <rdfs:Datatype>
          <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
          <owl:withRestrictions>
            <rdf:Description>
              <rdf:first>
                <rdf:Description>
                  <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">7</xsd:minExclusive>
                </rdf:Description>
              </rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
          </owl:withRestrictions>
        </rdfs:Datatype>
      </owl:allValuesFrom>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Draw">
  <rdfs:subClassOf rdf:resource="#Match"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Match"/>
        <owl:Class>
